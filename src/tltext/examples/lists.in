//List Operations
data List a = Cons a (List a) | ListTail;;

fun head.l [l : [type <- "List"]] = l!arg0;;
fun tail.l [l : [type <- "List"]] = l!arg1;;

var headv [arg0 : [type <- "List"]] = #!arg0!arg0;;
var tailv [arg0 : [type <- "List"]] = #!arg0!arg1;;

